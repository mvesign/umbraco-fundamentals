{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "6075595e-1796-483b-9b69-db8fb6245e66",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "uSync": {
    "Settings": {
      "AddOnPing": false,
      "ExportAtStartup": "None",
      "ExportOnSave": "None",
      "ImportAtStartup": "None",
      "ImportOnFirstBoot": false,
      "FailOnMissingParent": true,
      "FailOnDuplicates": true,
      "UiEnabledGroups": "Settings",
      "Folders": [ "uSync/UmbracoFundamentals" ]
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/UmbracoFundamentals.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}