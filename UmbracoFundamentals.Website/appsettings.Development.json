{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      }
    }
  },
  "uSync": {
    "Settings": {
      "ExportOnSave": "Settings",
      "ImportAtStartup": "Settings",
      "ImportOnFirstBoot": true,
      "FirstBootGroup": "Settings"
    },
    "Sets": {
      "Default": {
        "Handlers": {
          "DictionaryHandler": {
            "CreateOnly": false
          }
        }
      }
    }
  }
}
